# API Configuration
REACT_APP_BACKEND_URL=https://your-api-domain.railway.app/api/1.0
REACT_APP_SOCKET_SERVER=https://your-api-domain.railway.app

# Build Configuration
NODE_OPTIONS=--max_old_space_size=4096

# Third-party Services (replace with your own keys)
REACT_APP_ABLY_KEY=your_ably_key_here
REACT_APP_ABLY_API=your_ably_api_key_here
REACT_APP_GET_STREAM_KEY=your_stream_key_here
REACT_APP_GET_STREAM_SECRET=your_stream_secret_here

# Environment
REACT_APP_ENVIRONMENT=production